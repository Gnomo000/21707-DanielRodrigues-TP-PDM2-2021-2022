        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/woods_2</item>
        <item name="colorPrimaryVariant">@color/woods_2</item>
        <item name="colorOnPrimary">@color/white</item>
        <item name="android:windowBackground">#DCD0C3</item>
        <!-- Secondary brand color. -->
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="android:navigationBarColor">@color/woods_2</item>
    </style>
</resources>